
define parray
	if $argc == 0 || $argc == 1
		help parray
	end
	if $argc == 2
		set $i = 0
		while $i < $arg1
			printf "%d ", (int)$arg0[$i]
			set $i += 1
		end
		printf "\n"
	end
end

document parray
	Prints array information.
	Syntax: parray <array_name> <size>
	Syntax: parray <array_name> <start> <end>
end

